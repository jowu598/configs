plugins=(vi-mode)

alias xrandr_mirror=' xrandr --fb 1920x1080 --output eDP-1-1 --output HDMI-0 --mode 1920x1080 --scale 1x1 --same-as eDP-1-1'


lll() {
 ls -lt | awk '{print $NF}'
}
alias m='make >&log;err'
alias w='~/w'
